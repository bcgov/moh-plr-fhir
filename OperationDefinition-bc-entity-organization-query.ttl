<http://hlth.gov.bc.ca/fhir/provider/OperationDefinition/bc-entity-organization-query> a fhir:OperationDefinition ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "bc-entity-organization-query"] ; # 
  fhir:text [
     fhir:status [ fhir:v "extensions" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\"><p>URL: [base]/Organization/$entityQuery</p><p>URL: [base]/Organization/[id]/$entityQuery</p><p>Parameters</p><table class=\"grid\"><tr><td><b>Use</b></td><td><b>Name</b></td><td><b>Scope</b></td><td><b>Cardinality</b></td><td><b>Type</b></td><td><b>Binding</b></td><td><b>Documentation</b></td></tr><tr><td>IN</td><td>identifier</td><td/><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#string\">string</a><br/>(<a href=\"http://hl7.org/fhir/R4/search.html#string\">string</a>)</td><td/><td><div><p>String, representing the full identifier value with system and value, e.g. identifier=[system]|[value]</p>\n</div></td></tr><tr><td>IN</td><td>name</td><td/><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#string\">string</a><br/>(<a href=\"http://hl7.org/fhir/R4/search.html#string\">string</a>)</td><td/><td><div><p>Search for organizations with the specified name, mandatory if no identifier specified. May use trailing wildcards, e.g. Clinic*</p>\n</div></td></tr><tr><td>IN</td><td>description</td><td/><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#string\">string</a><br/>(<a href=\"http://hl7.org/fhir/R4/search.html#string\">string</a>)</td><td/><td><div><p>Search for organizations with the specified description, mapping to BCOrganization.alias. May use trailing wildcards, e.g. Clinic*</p>\n</div></td></tr><tr><td>IN</td><td>type</td><td/><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#string\">string</a><br/>(<a href=\"http://hl7.org/fhir/R4/search.html#token\">token</a>)</td><td/><td><div><p>Search for organizations with the specified role type code, mandatory if no identifier specified, mapping to Organization.type</p>\n</div></td></tr><tr><td>IN</td><td>address-line1</td><td/><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#string\">string</a><br/>(<a href=\"http://hl7.org/fhir/R4/search.html#string\">string</a>)</td><td/><td><div><p>Search will run on Physical address, then Mailing address if no match was found on. May use trailing wildcards</p>\n</div></td></tr><tr><td>IN</td><td>address-city</td><td/><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#string\">string</a><br/>(<a href=\"http://hl7.org/fhir/R4/search.html#string\">string</a>)</td><td/><td><div><p>Search for organizations with the specified city.</p>\n</div></td></tr><tr><td>IN</td><td>withHistory</td><td/><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#boolean\">boolean</a></td><td/><td><div><p>true or false, The withHistory parameter instructs PLR to search through historical records for matching attributes. Only the current data is returned.</p>\n</div></td></tr><tr><td>OUT</td><td>bundle</td><td/><td>1..1</td><td><a href=\"http://hl7.org/fhir/R4/bundle.html\">Bundle</a></td><td/><td><div><p>The Response Bundle with the OperationOutcome and the list of BCOrganizationBundle found by the search.</p>\n</div></td></tr></table></div>"
  ] ; # 
  fhir:url [ fhir:v "http://hlth.gov.bc.ca/fhir/provider/OperationDefinition/bc-entity-organization-query"^^xsd:anyURI] ; # 
  fhir:version [ fhir:v "1.0.0"] ; # 
  fhir:name [ fhir:v "OrganizationEntityQuery"] ; # 
  fhir:title [ fhir:v "Query for an organization without returning associations"] ; # 
  fhir:status [ fhir:v "draft"] ; # 
  fhir:kind [ fhir:v "query"] ; # 
  fhir:date [ fhir:v "2024-05-31T19:26:05+00:00"^^xsd:dateTime] ; # 
  fhir:publisher [ fhir:v "BC Ministry of Health"] ; # 
  fhir:contact ( [
     fhir:name [ fhir:v "BC Ministry of Health" ] ;
     fhir:telecom ( [
       fhir:system [ fhir:v "url" ] ;
       fhir:value [ fhir:v "https://www2.gov.bc.ca/gov/content/governments/organizational-structure/ministries-organizations/ministries/health" ]
     ] )
  ] ) ; # 
  fhir:description [ fhir:v "This operation is used to query for an organization and will return the information for that organization without following associations to other providers, organizations, or facilities."] ; # 
  fhir:jurisdiction ( [
     fhir:coding ( [
       fhir:system [ fhir:v "urn:iso:std:iso:3166"^^xsd:anyURI ] ;
       fhir:code [ fhir:v "CA" ] ;
       fhir:display [ fhir:v "Canada" ]
     ] )
  ] ) ; # 
  fhir:code [ fhir:v "entityQuery"] ; # 
  fhir:resource ( [ fhir:v "Organization"] ) ; # 
  fhir:system [ fhir:v "false"^^xsd:boolean] ; # 
  fhir:type [ fhir:v "true"^^xsd:boolean] ; # 
  fhir:instance [ fhir:v "true"^^xsd:boolean] ; # 
  fhir:parameter ( [
     fhir:name [ fhir:v "identifier" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "0"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "String, representing the full identifier value with system and value, e.g. identifier=[system]|[value]" ] ;
     fhir:type [ fhir:v "string" ] ;
     fhir:searchType [ fhir:v "string" ]
  ] [
     fhir:name [ fhir:v "name" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "0"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "Search for organizations with the specified name, mandatory if no identifier specified. May use trailing wildcards, e.g. Clinic*" ] ;
     fhir:type [ fhir:v "string" ] ;
     fhir:searchType [ fhir:v "string" ]
  ] [
     fhir:name [ fhir:v "description" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "0"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "Search for organizations with the specified description, mapping to BCOrganization.alias. May use trailing wildcards, e.g. Clinic*" ] ;
     fhir:type [ fhir:v "string" ] ;
     fhir:searchType [ fhir:v "string" ]
  ] [
     fhir:name [ fhir:v "type" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "0"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "Search for organizations with the specified role type code, mandatory if no identifier specified, mapping to Organization.type" ] ;
     fhir:type [ fhir:v "string" ] ;
     fhir:searchType [ fhir:v "token" ]
  ] [
     fhir:name [ fhir:v "address-line1" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "0"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "Search will run on Physical address, then Mailing address if no match was found on. May use trailing wildcards" ] ;
     fhir:type [ fhir:v "string" ] ;
     fhir:searchType [ fhir:v "string" ]
  ] [
     fhir:name [ fhir:v "address-city" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "0"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "Search for organizations with the specified city." ] ;
     fhir:type [ fhir:v "string" ] ;
     fhir:searchType [ fhir:v "string" ]
  ] [
     fhir:name [ fhir:v "withHistory" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "0"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "true or false, The withHistory parameter instructs PLR to search through historical records for matching attributes. Only the current data is returned." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:name [ fhir:v "bundle" ] ;
     fhir:use [ fhir:v "out" ] ;
     fhir:min [ fhir:v "1"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
     fhir:documentation [ fhir:v "The Response Bundle with the OperationOutcome and the list of BCOrganizationBundle found by the search." ] ;
     fhir:type [ fhir:v "Bundle" ]
  ] ) . # 

